    $(".navigation > li:eq(2)").parent().css('opacity', '0.3');
    $('.servicesBtn').closest('div').css('background-color', 'rgba(255, 0, 0, 0.2)');
    $('.servicesBtn').closest('div').css('border', '2px solid red');
})